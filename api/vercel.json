{
  "version": 2,
  "routes": [
    {
      "src": "/api/calendar",
      "dest": "/api/calendar.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
        { "key": "Cache-Control", "value": "s-maxage=300, stale-while-revalidate" }
      ]
    }
  ]
}
